@random_walk:
options := maze.can.move.forward
options += maze.can.move.left
options += maze.can.move.right
if options = 0   # dead end
 turn_left :~ 2
 if turn_left = 1
  maze.turn.left()
 else
  maze.turn.right()
 endif
endif
random_option :~ options
if maze.can.move.left = 1
 if random_option = 0
  maze.turn.left()
 endif
 random_option--
endif
if maze.can.move.right = 1
 if random_option = 0
  maze.turn.right()
 endif
endif
maze.move.forward()
